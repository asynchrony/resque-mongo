<% if key = params[:key] %>

  <p class='sub'>
    Showing <%= start = params[:start].to_i %> to <%= start + 20 %> of <b><%=size = mongo_get_size(key) %></b>
  </p>

    
  <h1>Elements in Collection <%= key %></h1>
  <table>
    <% for row in mongo_get_value_as_array(key, start) %>
    <tr>
      <td>
        <%= row.inspect %>
      </td>
    </tr>
    <% end %>
  </table>

<%= partial :next_more, :start => start, :size => size %>
<% end %>
